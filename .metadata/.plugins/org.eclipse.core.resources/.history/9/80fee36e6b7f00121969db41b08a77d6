 * This class is used to declare methods that are generic and used throughout the project
 * Rule: Do not use specific methods that pertain to a certain class, try to generalize them
 * for the greater good of team
 */
var indexZero = 0;
function clearArray(thisArray){
	thisArray.splice(indexZero, thisArray.length);
}

function sortNumericalArrayAscending(thisArray){
	thisArray.sort(function(a,b){return a-b});
}

function sortNumericalArrayDescending(thisArray){
	thisArray.sort(function(a,b){return b-a});
}

function sortStringArray(thisArray){
	thisArray.sort();
}

function sortListAscending(thisList,thisType){
	
}

function sortListDescending(thisList,thisType){
	
}
